<script>
    var y = 0;
    var x;
    $(document).on('click', '.UnSelectedSecOptionSelect .itemCheckBox input', function () {

        $(this).parent().parent().parent().parent().parent(".SecOptionSelect");
        $(this).parent().parent().parent().parent().parent(".SecOptionSelect").addClass("SecOptionSelectDone");
        if (y == 0) {
            $(".SelectedSecOptionSelect").append("<h5 class='TitleSelect'>You have selected the flight from Cairo to Kuwait</h5>");
        }
        y++;
        x = $(this).parent().parent().parent().parent().parent(".SecOptionSelect");
        $(".SelectedSecOptionSelect").append(x);
        $(".UnSelectedSecOptionSelect .SecOptionSelect.SecOptionSelectDone").remove();

        $(".SelectedSecOptionSelect .BookChoose").hide();
        $(".SelectedSecOptionSelect .BookChooseGray").show();
    });
    $(document).on('click', '.SelectedSecOptionSelect .itemCheckBox input', function () {
        debugger;
        $(this).parent().parent().parent().parent().parent(".SecOptionSelect").removeClass("SecOptionSelectDone");
        y--;
        if (y == 0) {
            $(".SelectedSecOptionSelect .TitleSelect").remove();
        }
        x = $(this).parent().parent().parent().parent().parent(".SecOptionSelect");
        $(".UnSelectedSecOptionSelect").append(x);
        $(this).parent().parent().parent().parent().parent(".SelectedSecOptionSelect .SecOptionSelect").remove();
        $(".UnSelectedSecOptionSelect .BookChoose").show();
        $(".UnSelectedSecOptionSelect .BookChoose.BookChooseGray").hide();
    })

</script>
<script>

    $(function () {
        $("#slider-range").slider({
